    </security-role>
    <security-role>
        <description/>
        <role-name>GERENTE_TRANSPORTACION</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>COORDINADORA_DE_LOGISTICA</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>LOGISTICA_TRANSPORTACION</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>GUIA</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>DIRECTOR_ADDONS</role-name>
    </security-role>
</web-app>
