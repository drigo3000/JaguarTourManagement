    </security-role>
    <security-role>
        <description/>
        <role-name>ADMINISTRACION_CENTRO_DE_NEGOCIOS</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>SUPERVISOR_CENTRO_NEGOCIOS</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>CAJERO_GENERAL</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>JEFE DE COSTOS</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>GERENTE_RESERVACIONES</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>OPERACIONES</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>COBRANZA</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>SUPERVISOR_RESERVACIONES</role-name>
    </security-role>
    <security-role>
        <description/>
        <role-name>DIRECTOR_OPERATIVO</role-name>
    </security-role>
    <security-role>
        <description/>
